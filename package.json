{
  "name": "zapdns",
  "version": "0.1.1-beta",
  "engine": {
  },
  "description": "ZapDNS is a cross-platform tool for quickly managing and switching DNS settings to save time and optimize network configurations.",
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "ncc build -o dist && sh build.sh",
