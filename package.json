  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/robin-deruyck/zapDNS"
  },
  "bugs": {
    "url": "https://github.com/robin-deruyck/zapDNS/issues"
  },
  "homepage": "https://github.com/yourusername/zapdns#readme",
  "dependencies": {
    "chalk": "^4.1.0",
    "inquirer": "^12.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@vercel/ncc": "^0.38.2",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
